{
  "name": "mehrshaddarzi/wp-cli-reference-command",
  "description": "WordPress Code Reference Package for WP-CLI.",
  "type": "wp-cli-package",
  "homepage": "https://github.com/mehrshaddarzi/wp-cli-reference-command",
  "license": "MIT",
  "authors": [
    {
      "name": "Mehrshad Darzi",
      "email": "mehrshad198@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "": "src/"
    },
    "files": [
      "command.php"
    ]
  },
  "require": {
    "php": "^5.5 || ^7.0",
    "wp-packagist/wp-cli-helper": "dev-master",
    "ext-json": "*",
    "ext-dom": "*"
  },
  "require-dev": {
    "wp-cli/wp-cli": "^2.0",
    "phpunit/phpunit": "^4.8"
  },
  "extra": {
    "commands": [
      "reference"
    ]
  },
  "support": {
    "issues": "https://github.com/mehrshaddarzi/wp-cli-reference-command/issues"
  },
  "scripts": {
    "test": "phpunit"
  }
}
